<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
         xmlns="http://java.sun.com/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>BaseQuery</servlet-name>
        <servlet-class>tach.actions.BaseQuery</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JsonQuery</servlet-name>
        <servlet-class>tach.actions.JsonQuery</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JsonQuery</servlet-name>
        <url-pattern>/actions/jsonquery</url-pattern>
    </servlet-mapping>
</web-app>
